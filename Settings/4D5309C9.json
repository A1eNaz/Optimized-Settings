{
    "APU": {
        "apu_max_queued_frames": 8,
        "use_dedicated_xma_thread": false,
        "use_new_decoder": true
    },
    "D3D12": {
        "d3d12_readback_resolve": false
    },
    "Display": {
        "postprocess_ffx_cas_additional_sharpness": 1.000,
        "postprocess_ffx_fsr_sharpness_reduction": 0.200,
        "postprocess_scaling_and_sharpening": "fsr"
    },
    "GPU": {
        "framerate_limit": 120,
        "gpu_allow_invalid_fetch_constants": true,
        "vsync": false
    },
    "Storage": {
        "mount_cache": true,
        "mount_scratch": true 
    }
}
